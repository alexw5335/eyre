namespace main

proc main {
	crc32 ecx, al
	crc32 rcx, al
	crc32 ecx, ax
	crc32 ecx, eax
	crc32 rcx, rax

	crc32 ecx, byte [rax]
	crc32 rcx, byte [rax]
	crc32 ecx, [rax]
	crc32 rcx, [rax]
	crc32 ecx, word [rax]
	crc32 ecx, dword [rax]

	enqcmd ecx, [rax]
	enqcmd rcx, [rax]
	enqcmds ecx, [rax]
	enqcmds rcx, [rax]
	movdir64b ecx, [rax]
	movdir64b rcx, zword [rax]
	umonitor ecx
	umonitor rcx

	shld rcx, rax, 1
	shld rcx, rax, cl
	shld [rax], rcx, 1
	shld [rax], rcx, cl
}